# Cambiar los elementos de una lista
import os;
os.system("cls")

nums = [10, 9, 8.5, 6.5, 9.8, 7, 5, 6.2, 9.5]

print(nums, len(nums)) #len para saber cuantos elementos hay en la lista
print(type(nums))
nums[0]=7
nums[1]=7
print(nums)
nums[2:5] = [9.0,9.0,9.0]
print(nums)
nums[-1] = 6.0
print(nums)
nums[6] = nums[6] + 5 
print(nums)
