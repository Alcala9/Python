# Determina al mayor de 3 numeros

import os
os.system('cls')

def mayor(n1,n2,n3):
    may=0
    if n1>n2 and n1>n3:
        may = n1
    elif n2>n1 and n2>n3:
        may = n2
    else:
        may = n3
    return may

print("Dame 3 numeros :")
a,b,c = float(input()), float(input()), float(input())
may = mayor(a,b,c)
print(f"El mayor de los 3 numeros es: {may}")
