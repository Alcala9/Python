# Remover elementos de la lista
import os
os.system('cls')

nums = [1, 3, 5, 7, 9, 11, 99, 15, 88, 19, 100]
print(nums, len(nums))

nums.remove(99)           #Con remove quitamos un elemento (Escribimos lo que queremos eliminar)
print(nums, len(nums))

num=nums.pop(8)           #Con pop quitamos un elemento (Escribimos lo que queremos sacar)
print(nums, len(nums))    

num=nums.pop()            #Con pop (Sin decir la posicion) quitamos el ultimo elemento 
print(nums, len(nums))

nums.clear()              #Con clear limpiamos TOODA la lista
print(nums, len(nums))