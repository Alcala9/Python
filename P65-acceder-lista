# Acceder a los elementos de una lista
import os; os.system('cls')
#       0      1   2     3   4    5    6    7    8
nums = [100, 123, 456, 222, 999, 895, 325, 234, 322, 988]
#            -8   -7   -6   -5   -4   -3   -2   -1   -0 

print(nums)
print(nums[0])
print(nums[-1])
print(nums[4])
print(nums[1:5])
print(nums[:4])
print(nums[3:])
print(nums[-3:])
