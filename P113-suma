# Calcula una lista de numeros usando una funcion
import os
os.system('cls')

def suma(lista):
    s = 0
    for n in lista:
        s += n
    return s

# Programa principal
lista = []
while True:
    n = int(input("Numero ? "))
    if n==-1: break
    lista.append(n)
print(lista, len(lista))
# lista = [10, 20, 30, 40, 50]
res = suma(lista)
print('La suma es ', res)